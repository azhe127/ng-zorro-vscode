{
 
     "root root": {
        "prefix": "nz-root",
        "body": "<ng-root>$1</ng-root>",
        "description": "根组件只能引入一次，所有ng-zorro组件都应该包裹在下面",
        "scope": ""
    }

    , "general button": {
        "prefix": "nz-button",
        "body": "<button nz-button (click)=\"${2}\" [nzType]=\"'${3|primary,dashed,danger,default|}'\"${4: [nzSize]=\"'${5|small,large|}'\"}>\n    <span>$1</span>\n</button>\n$0",
        "description": "按钮（只含type&size）",
        "scope": ""
    }

    , "general button full": {
        "prefix": "nz-button-full",
        "body": "<button nz-button (click)=\"${2}\" [nzType]=\"'${3|primary,dashed,danger,default|}'\"${4: [nzShape]=\"'circle'\"}${5: [nzSize]=\"'${6|small,large|}'\"}${7: [nzLoading]=\"true\"}${8: [nzGhost]=\"true\"}>\n    <span>$1</span>\n</button>\n$0",
        "description": "一个完整指令的按钮",
        "scope": ""
    }

    , "general button group": {
        "prefix": "nz-button-group",
        "body": "<nz-button-group${1: [nzSize]=\"'${2|small,large|}'\"}>\n    <button nz-button (click)=\"${4}\"${5: [nzType]=\"'${6|primary,dashed,danger,default|}'\"}>\n        <span>$3</span>\n    </button>\n    $0\n</nz-button-group>",
        "description": "按钮组",
        "scope": ""
    }

    , "general button icon text left": {
        "prefix": "nz-button-icon-text-left",
        "body": "<button nz-button (click)=\"${3}\" [nzType]=\"'${4|primary,dashed,danger,default|}'\"${5: [nzShape]=\"'circle'\"}>\n    <span>${1}</span><i class=\"anticon anticon-${2}\"></i>\n</button>\n$0",
        "description": "文本在左边的图标按钮",
        "scope": ""
    }

    , "general button icon text right": {
        "prefix": "nz-button-icon-text-right",
        "body": "<button nz-button (click)=\"${3}\" [nzType]=\"'${4|primary,dashed,danger,default|}'\"${5: [nzShape]=\"'circle'\"}>\n    <i class=\"anticon anticon-${2}\"></i><span>${1}</span>\n</button>\n$0",
        "description": "文本在右边的图标按钮",
        "scope": ""
    }

    , "general button icon": {
        "prefix": "nz-button-icon",
        "body": "<button nz-button (click)=\"${2}\" [nzType]=\"'${3|primary,dashed,danger,default|}'\"${4: [nzShape]=\"'circle'\"}>\n    <i class=\"anticon anticon-${1}\"></i>\n</button>\n$0",
        "description": "图标按钮",
        "scope": ""
    }

    , "general icon": {
        "prefix": "nz-icon",
        "body": "<i class=\"anticon anticon-${1|left,right,search|}\"></i>",
        "description": "Icon图标",
        "scope": ""
    }

    , "layout grid col responsive": {
        "prefix": "nz-grid-col-responsive",
        "body": "<div nz-col [nzXs]=\"${1:1-24}\" [nzSm]=\"${2:1-24}\" [nzMd]=\"${3:1-24}\" [nzLg]=\"${4:1-24}\" [nzXl]=\"${5:1-24}\"${6: [nzOffset]=\"${7:1-24}\"}>\n    $0\n</div>",
        "description": "栅格：一列（响应式）",
        "scope": ""
    }

    , "layout grid col": {
        "prefix": "nz-grid-col",
        "body": "<div nz-col [nzSpan]=\"${1:1-24}\"${2: [nzOffset]=\"${3:1-24}\"}>\r\n    $4\r\n</div>",
        "description": "栅格：一列（含：nzSpan、nzOffset）",
        "scope": ""
    }

    , "layout grid": {
        "prefix": "nz-grid",
        "body": "<div nz-row${1: [nzGutter]=\"${2|8,16,24,32,64|}\"}>\n    <div nz-col [nzSpan]=\"${3:1-24}\"${4: [nzOffset]=\"${5:1-24}\"}>\n        $0\n    </div>\n</div>",
        "description": "栅格：一行一列",
        "scope": ""
    }

    , "layout grid row flex": {
        "prefix": "nz-grid-row-flex",
        "body": "<div nz-row [nzType]=\"'flex'\"${1: [nzJustify]=\"'${2|start,center,end,space-between,space-around|}'\"}${3: [nzAlign]=\"'${4|top,middle,bottom|}'\"}>\n    $0\n</div>",
        "description": "栅格：一行flex（含：nzAlign、nzJustify）",
        "scope": ""
    }

    , "layout grid row": {
        "prefix": "nz-grid-row",
        "body": "<div nz-row${1: [nzGutter]=\"${2|8,16,24,32,64|}\"}>\n    $0\n</div>",
        "description": "栅格：一行",
        "scope": ""
    }

    , "layout layout": {
        "prefix": "nz-layout",
        "body": "<nz-layout>\n    <nz-header>\n        $0\n    </nz-header>\n    <nz-content>\n\n    </nz-content>\n    <nz-footer>\n\n    </nz-footer>\n</nz-layout>",
        "description": "上中下 Layout 布局",
        "scope": ""
    }

    , "layout layout sider": {
        "prefix": "nz-layout-sider",
        "body": "<nz-sider [nzCollapsible]=\"true\"${1: [nzWidth]=\"${2:200}\"}${3: [nzCollapsedWidth]=\"${4:64}\"}>\n    $0\n</nz-sider>",
        "description": "侧边栏",
        "scope": ""
    }

    , "navigation breadcrumb": {
        "prefix": "nz-breadcrumb",
        "body": "<nz-breadcrumb${1: [nzSeparator]=\"'${2:>}'\"}>\n    <nz-breadcrumb-item>\n        <a [routerLink]=\"['/${3}']\">${4}</a>\n    </nz-breadcrumb-item>\n    $0\n</nz-breadcrumb>",
        "description": "面包屑",
        "scope": ""
    }

    , "navigation breadcrumb icon": {
        "prefix": "nz-breadcrumb-icon",
        "body": "<nz-breadcrumb-item>\n    <a [routerLink]=\"['/${1}']\"><i class=\"anticon anticon-${2:home}\"></i>${3:<span>${4:首页}</span>}</a>\n</nz-breadcrumb-item>\n$0",
        "description": "面包屑ICON图标",
        "scope": ""
    }

    , "navigation dropdown button full": {
        "prefix": "nz-dropdown-button-full",
        "body": "<nz-dropdown-button${2: [nzType]=\"'${3|primary,dashed,danger,default|}'\"}${4: [nzSize]=\"'${5|small,large|}'\"}\n    ${6: [nzTrigger]=\"'${7|click,hover|}'\"}${8: [nzPlacement]=\"'${9|bottomLeft,bottomCenter,bottomRight,topLeft,topCenter,topRight|}'\"}>\n    <button nz-button nz-dropdown>${1}</button>\n    <ul nz-menu>\n        <li nz-menu-item>${10}</li>\n        $0\n    </ul>\n</nz-dropdown-button>",
        "description": "按钮式下拉菜单（含一个菜单）",
        "scope": ""
    }

    , "navigation dropdown button": {
        "prefix": "nz-dropdown-button",
        "body": "<nz-dropdown-button${2: [nzType]=\"'${3|primary,dashed,danger,default|}'\"}${4: [nzSize]=\"'${5|small,large|}'\"}\n    ${6: [nzTrigger]=\"'${7|click,hover|}'\"}${8: [nzPlacement]=\"'${9|bottomLeft,bottomCenter,bottomRight,topLeft,topCenter,topRight|}'\"}>\n    <button nz-button nz-dropdown>${1}</button>\n    $0\n</nz-dropdown-button>",
        "description": "按钮式下拉菜单",
        "scope": ""
    }

    , "navigation dropdown": {
        "prefix": "nz-dropdown",
        "body": "<nz-dropdown${2: [nzTrigger]=\"'${3|click,hover|}'\"}${4: [nzPlacement]=\"'${5|bottomLeft,bottomCenter,bottomRight,topLeft,topCenter,topRight|}'\"}>\n    <a class=\"ant-dropdown-link\" nz-dropdown>\n        ${1} <i class=\"anticon anticon-down\"></i>\n    </a>\n    $0\n</nz-dropdown>",
        "description": "下拉菜单",
        "scope": ""
    }

    , "navigation dropdown full": {
        "prefix": "nz-dropdown-full",
        "body": "<nz-dropdown${2: [nzTrigger]=\"'${3|click,hover|}'\"}${4: [nzPlacement]=\"'${5|bottomLeft,bottomCenter,bottomRight,topLeft,topCenter,topRight|}'\"}>\n    <a class=\"ant-dropdown-link\" nz-dropdown>\n        ${1} <i class=\"anticon anticon-down\"></i>\n    </a>\n    <ul nz-menu>\n        <li nz-menu-item>${6}</li>\n        $0\n    </ul>\n</nz-dropdown>",
        "description": "下拉菜单（含一个菜单）",
        "scope": ""
    }

    , "navigation menu": {
        "prefix": "nz-menu",
        "body": "<ul nz-menu${1: [nzMode]=\"'${2|vertical,horizontal,inline|}'\"}${3: [nzTheme]=\"'${4|light,dark|}'\"}>\n    $0\n</ul>",
        "description": "导航菜单",
        "scope": ""
    }

    , "navigation menu group": {
        "prefix": "nz-menu-group",
        "body": "<li nz-menu-group>\n    <span title>${1}</span>\n    $0\n</li>",
        "description": "分组菜单",
        "scope": ""
    }

    , "navigation menu item divider": {
        "prefix": "nz-menu-item-divider",
        "body": "<li nz-menu-divider></li>\n$0",
        "description": "菜单项分割线",
        "scope": ""
    }

    , "navigation menu item": {
        "prefix": "nz-menu-item",
        "body": "<li nz-menu-item>${1}</li>\n$0",
        "description": "菜单项",
        "scope": ""
    }

    , "navigation menu submenu": {
        "prefix": "nz-menu-submenu",
        "body": "<li nz-submenu${1: [nzOpen]=\"true\"}>\n    <span title>${2}</span>\n    <ul>\n        $0\n    </ul>\n</li>",
        "description": "子菜单",
        "scope": ""
    }

    , "navigation pagination": {
        "prefix": "nz-pagination",
        "body": "<nz-pagination [nzPageIndex]=\"${1}\" [nzTotal]=\"${2}\" [nzPageSize]=\"${3}\"${4: [nzSize]=\"'small'\"}></nz-pagination>",
        "description": "分页",
        "scope": ""
    }

    , "navigation steps": {
        "prefix": "nz-steps",
        "body": "<nz-steps [(nzCurrent)]=\"${1:current}\"${2: [nzStatus]=\"'${3|wait,process,finish,error|}'\"}${4: [nzSize]=\"'small'\"}${5: [nzDirection]=\"'${6|horizontal,vertical|}'\"}${7: [nzProgressDot]=\"true\"}>\n    <nz-step [nzTitle]=\"'${8}'\"></nz-step>\n    $0\n</nz-steps>\n<div class=\"steps-content\">\n\n</div>",
        "description": "整体步骤条",
        "scope": ""
    }

    , "navigation steps step icon": {
        "prefix": "nz-steps-step-icon",
        "body": "<nz-step [nzTitle]=\"'${1}'\" [nzDescription]=\"'${2}'\"${3: [nzStatus]=\"'${4|wait,process,finish,error|}'\"}>\n    <ng-template #nzIcon>$6</ng-template>\n</nz-step>",
        "description": "步骤条内的每一个步骤（自定义图标）",
        "scope": ""
    }

    , "navigation steps step": {
        "prefix": "nz-steps-step",
        "body": "<nz-step [nzTitle]=\"'${1}'\" [nzDescription]=\"'${2}'\"${3: [nzStatus]=\"'${4|wait,process,finish,error|}'\"}></nz-step>",
        "description": "步骤条内的每一个步骤",
        "scope": ""
    }

    , "data-entry checkbox all": {
        "prefix": "nz-checkbox-all",
        "body": "<label nz-checkbox [(ngModel)]=\"${1:allChecked}\"${2: (ngModelChange)=\"${3}\"} [nzIndeterminate]=\"indeterminate\">\r\n    <span>${4}</span>\r\n</label>",
        "description": "多选框（全选）",
        "scope": ""
    }

    , "data-entry checkbox": {
        "prefix": "nz-checkbox",
        "body": "<label nz-checkbox [(ngModel)]=\"${1}\"${2: [nzDisabled]=\"${3:isDisabled}\"}>\r\n    <span>${4}</span>\r\n</label>",
        "description": "多选框",
        "scope": ""
    }

    , "data-entry checkbox group": {
        "prefix": "nz-checkbox-group",
        "body": "<nz-checkbox-group [(ngModel)]=\"${1}\"${2: (ngModelChange)=\"${3}\"}></nz-checkbox-group>",
        "description": "多选框组",
        "scope": ""
    }

    , "data-entry datepicker": {
        "prefix": "nz-datepicker",
        "body": "<nz-datepicker [(ngModel)]=\"${1:begin}\" [nzPlaceHolder]=\"'${2:请选择日期}'\"${3: [nzSize]=\"'${4|large,small|}'\"}></nz-datepicker>",
        "description": "日期选择框",
        "scope": ""
    }

    , "data-entry datepicker format": {
        "prefix": "nz-datepicker-format",
        "body": "<nz-datepicker [(ngModel)]=\"${1:begin}\" [nzPlaceHolder]=\"'${2:请选择日期}'\"\n     [nzFormat]=\"'${3:YYYY-MM-DD HH:mm:ss}'\"${4: [nzSize]=\"'${5|large,small|}'\"}></nz-datepicker>",
        "description": "日期选择框（指定时间格式）",
        "scope": ""
    }

    , "data-entry datepicker time": {
        "prefix": "nz-datepicker-time",
        "body": "<nz-datepicker [(ngModel)]=\"${1:begin}\" [nzPlaceHolder]=\"'${2:请选择日期时间}'\"${3: [nzSize]=\"'${4|large,small|}'\"}\n     [nzShowTime]=\"true\" [nzFormat]=\"'${5:YYYY-MM-DD HH:mm:ss}'\"></nz-datepicker>",
        "description": "日期时间选择框",
        "scope": ""
    }

    , "data-entry form control responsive": {
        "prefix": "nz-form-control-responsive",
        "body": "<div nz-form-control nz-col [nzXs]=\"${1:1-24}\" [nzSm]=\"${2:1-24}\" [nzMd]=\"${3:1-24}\" [nzLg]=\"${4:1-24}\" [nzXl]=\"${5:1-24}\"${6: [nzHasFeedback]=\"'true'\"}${7: [nzValidateStatus]=\"${8|success,warning,error,validating|}\"}>\n    $0\n</div>",
        "description": "表单控制区域，通常用于放置input，select等组件（响应式）",
        "scope": ""
    }

    , "data-entry form control": {
        "prefix": "nz-form-control",
        "body": "<div nz-form-control nz-col [nzSpan]=\"${1:1-24}\"${2: [nzHasFeedback]=\"'true'\"}${3: [nzValidateStatus]=\"${4|success,warning,error,validating|}\"}>\n    $0\n</div>",
        "description": "表单控制区域，通常用于放置input，select等组件",
        "scope": ""
    }

    , "data-entry form": {
        "prefix": "nz-form",
        "body": "<form nz-form [formGroup]=\"${1:groupName}\" (ngSubmit)=\"_submitForm(\\$event, ${1:groupName}.value)\"${2: [nzLayout]=\"'${3|horizontal,vertical,inline|}'\"}>\n    $0\n</form>",
        "description": "原生form的增强，用于加强form的样式",
        "scope": ""
    }

    , "data-entry form explain": {
        "prefix": "nz-form-explain",
        "body": "<div nz-form-explain>$0</div>",
        "description": "用于显示提示信息，会根据当前`nzValidateStatus`显示不同的颜色",
        "scope": ""
    }

    , "data-entry form extra": {
        "prefix": "nz-form-extra",
        "body": "<div nz-form-extra>$0</div>",
        "description": "用于显示表单额外提示信息",
        "scope": ""
    }

    , "data-entry form item required": {
        "prefix": "nz-form-item-required",
        "body": "<label for=\"${1:id}\" nz-form-item-required>${2:text}</label>$0",
        "description": "给label添加require样式",
        "scope": ""
    }

    , "data-entry form item": {
        "prefix": "nz-form-item",
        "body": "<div nz-form-item nz-row>\r\n    $0\r\n</div>",
        "description": "用于标定一个form item区域",
        "scope": ""
    }

    , "data-entry form label responsive": {
        "prefix": "nz-form-label-responsive",
        "body": "<div nz-form-label nz-col [nzXs]=\"${1:1-24}\" [nzSm]=\"${2:1-24}\" [nzMd]=\"${3:1-24}\" [nzLg]=\"${4:1-24}\" [nzXl]=\"${5:1-24}\">\n    <label for=\"${6:id}\"${7: nz-form-item-required}>${8:text}</label>\n</div>\n$0",
        "description": "用于标定label区域（响应式）",
        "scope": ""
    }

    , "data-entry form label": {
        "prefix": "nz-form-label",
        "body": "<div nz-form-label nz-col [nzSpan]=\"${1:1-24}\">\r\n    <label for=\"${2:id}\"${3: nz-form-item-required}>${4:text}</label>\r\n</div>\r\n$0",
        "description": "用于标定label区域",
        "scope": ""
    }

    , "data-entry form split": {
        "prefix": "nz-form-split",
        "body": "<div nz-form-split>${1:\/}</div>",
        "description": "用于显示 `-` 分隔符",
        "scope": ""
    }

    , "data-entry input add on": {
        "prefix": "nz-input-add-on",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #addOnBefore>$5</ng-template>\n    <ng-template #addOnAfter>$6</ng-template>\n</nz-input>",
        "description": "带有前后置标签的输入框",
        "scope": ""
    }

    , "data-entry input": {
        "prefix": "nz-input",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}${5: [nzDisabled]=\"'${6|true,false|}'\"}></nz-input>",
        "description": "输入框",
        "scope": ""
    }

    , "data-entry input group": {
        "prefix": "nz-input-group",
        "body": "<nz-input-group${1: [nzCompact]=\"true\"}${2: [nzSize]=\"'${3|large,small|}'\"}>\n    $0\n</nz-input-group>",
        "description": "输入框组合",
        "scope": ""
    }

    , "data-entry input input add on after": {
        "prefix": "nz-input-input-add-on-after",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #addOnAfter>$5</ng-template>\n</nz-input>",
        "description": "带有前缀图标的输入框",
        "scope": ""
    }

    , "data-entry input input add on before": {
        "prefix": "nz-input-input-add-on-before",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #addOnBefore>$5</ng-template>\n</nz-input>",
        "description": "带有后缀图标的输入框",
        "scope": ""
    }

    , "data-entry input input prefix suffix": {
        "prefix": "nz-input-input-prefix-suffix",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #prefix>$5</ng-template>\n    <ng-template #suffix>$6</ng-template>\n</nz-input>",
        "description": "带有前后缀图标的输入框",
        "scope": ""
    }

    , "data-entry input number": {
        "prefix": "nz-input-number",
        "body": "<nz-input-number [(ngModel)]=\"${1:num}\" [nzMin]=\"${2:1}\" [nzMax]=\"${3:100}\" [nzStep]=\"${4:1}\"\n    ${5: [nzPlaceHolder]=\"'${6}'\"}${7: [nzSize]=\"'${8|large,small|}'\"}${9: [nzDisabled]=\"'${10|true,false|}'\"}></nz-input-number>",
        "description": "带数字输入框",
        "scope": ""
    }

    , "data-entry input prefix": {
        "prefix": "nz-input-prefix",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #prefix>$5</ng-template>\n</nz-input>",
        "description": "带有前缀图标的输入框",
        "scope": ""
    }

    , "data-entry input suffix": {
        "prefix": "nz-input-suffix",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzSize]=\"'${4|large,small|}'\"}>\n    <ng-template #suffix>$5</ng-template>\n</nz-input>",
        "description": "带有后缀图标的输入框",
        "scope": ""
    }

    , "data-entry input textarea": {
        "prefix": "nz-input-textarea",
        "body": "<nz-input [(ngModel)]=\"${1}\" [nzType]=\"'textarea'\" [nzRows]=\"'${2:4}'\" [nzPlaceHolder]=\"'${3}'\"></nz-input>$0",
        "description": "多行输入框",
        "scope": ""
    }

    , "data-entry radio button": {
        "prefix": "nz-radio-button",
        "body": "<label nz-radio-button [nzValue]=\"${1}\"${2: [nzDisabled]=\"${3}\"}>\r\n    <span>$4</span>\r\n</label>\r\n$0",
        "description": "按钮样式的单选组合",
        "scope": ""
    }

    , "data-entry radio": {
        "prefix": "nz-radio",
        "body": "<label nz-radio [nzValue]=\"${1}\"${2: [nzDisabled]=\"${3}\"}>\r\n    <span>$4</span>\r\n</label>\r\n$0",
        "description": "单选框（注：必须配合nz-radio-group使用）",
        "scope": ""
    }

    , "data-entry radio group": {
        "prefix": "nz-radio-group",
        "body": "<nz-radio-group [(ngModel)]=\"${1}\"${2: [nzSize]=\"'${3|large,small|}'\"}>\n    $0\n</nz-radio-group>",
        "description": "单选框组合，用于包裹一组 nz-radio/nz-radio-button",
        "scope": ""
    }

    , "data-entry rate": {
        "prefix": "nz-rate",
        "body": "<nz-rate [(ngModel)]=\"${1}\" [nzCount]=\"${2}\"${3: [nzDefaultValue]=\"${4}\"}${5: [nzAllowHalf]=\"true\"}${6: [nzDisabled]=\"true\"}></nz-rate>",
        "description": "评分组件",
        "scope": ""
    }

    , "data-entry select": {
        "prefix": "nz-select",
        "body": "<nz-select [(ngModel)]=\"${1}\" [nzPlaceHolder]=\"'${2}'\"${3: [nzAllowClear]=\"true\" }>\r\n    <nz-option \r\n        *ngFor=\"let ${5:item} of ${4:list}\" \r\n        [nzLabel]=\"${5:item}.label\"\r\n        [nzValue]=\"${5:item}.value\"\r\n        [nzDisabled]=\"${5:item}.disabled\">\r\n    </nz-option>\r\n</nz-select>\r\n$0",
        "description": "选择器",
        "scope": ""
    }

    , "data-entry select search": {
        "prefix": "nz-select-search",
        "body": "<nz-select [(ngModel)]=\"${1}\" [nzSearchChange]=\"${2}\" [nzMode]=\"'${3|multiple,tags|}'\"\n    [nzNotFoundContent]=\"'${4}'\" [nzPlaceHolder]=\"'${5}'\"${6: [nzAllowClear]=\"true\" }>\n    <nz-option\n        *ngFor=\"let ${8} of ${7}\"\n        [nzLabel]=\"${8}.label\"\n        [nzValue]=\"${8}.value\"\n        [nzDisabled]=\"${8}.disabled\">\n    </nz-option>\n</nz-select>\n$0",
        "description": "搜索框选择器",
        "scope": ""
    }

    , "data-entry slider": {
        "prefix": "nz-slider",
