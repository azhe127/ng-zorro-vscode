    , "feedback modal": {
        "prefix": "nz-modal",
        "body": "<nz-modal\n    [(nzVisible)]=\"isVisible\"\n    [nzConfirmLoading]=\"false|true\"\n    [nzTitle]=\"'标题'\"\n    [nzClosable]=\"false|true\"\n    [nzBody]=\"'这是内容'\"\n    [nzWidth]=\"520\"\n    [nzContent]=\"modalContent\"\n    [nzOkText]=\"'确定'\"\n    [nzCancelText]=\"'取消'\"\n    [nzMaskClosable]=\"false|true\"\n    [nzZIndex]=\"1000\"\n    [nzStyle]=\"{}\"\n    [nzWrapClassName]=\"''\"\n    (nzOnCancel)=\"handleCancel(\\$event)\"\n    (nzOnOk)=\"handleOk(\\$event)\">\n    <ng-template #modalContent>对话框的内容</ng-template>\n</nz-modal>\n$0",
        "description": "对话框（完整结构，因为需要放置HTML的情况时，认为是比较特别的需求；平常尽可能采用NzModalService服务调用方式）",
        "scope": ""
    }

    , "feedback popconfirm": {
        "prefix": "nz-popconfirm",
        "body": "<nz-popconfirm [nzTitle]=\"'${1:确定要删除吗？}'\" (nzOnConfirm)=\"${2:confirm(i)}\">\n    <a nz-popconfirm>${3:删除}</a>\n</nz-popconfirm>\n$0",
        "description": "气泡确认框",
        "scope": ""
    }

    , "feedback popconfirm full": {
        "prefix": "nz-popconfirm-full",
        "body": "<nz-popconfirm [nzTitle]=\"'${1:确定要删除吗？}'\"\n    [nzOkText]=\"'确定'\"\n    [nzCancelText]=\"'取消'\"\n    (nzOnConfirm)=\"confirm(i)\"\n    (nzOnCancel)=\"cancel(\\$event)\"\n    [nzCondition]=\"true\"\n    [nzPlacement]=\"'(top|left|right|bottom)(Top|Left|Right|Bottom)?'\"\n    [nzTrigger]=\"'hover|focus|click'\"\n    [(nzVisible)]=\"isVisible\"\n    (nzVisibleChange)=\"visibleChange()\"\n    [nzOverlayClassName]=\"''\"\n    [nzOverlayStyle]=\"{}\">\n    <a nz-popconfirm>按钮内容</a>\n</nz-popconfirm>\n$0",
        "description": "气泡确认框（完整结构）",
        "scope": ""
    }

    , "feedback progress circle": {
        "prefix": "nz-progress-circle",
        "body": "<nz-progress [(ngModel)]=\"${1}\" [nzStatus]=\"'${2|success,exception,active|}'\" [nzType]=\"'circle'\"\n     [nzStrokeWidth]=\"${3:6}\" [nzWidth]=\"${4:132}\"></nz-progress>\n$0",
        "description": "进度条（圈形）",
        "scope": ""
    }

    , "feedback progress": {
        "prefix": "nz-progress",
        "body": "<nz-progress [(ngModel)]=\"${1}\" [nzType]=\"'${2|circle,line|}'\"\n    [nzStatus]=\"'${3|success,exception,active|}'\"${4: [nzFormat]=\"function\"}${5: [nzShowInfo]=\"false\"}></nz-progress>\n$0",
        "description": "进度条（完整结构）",
        "scope": ""
    }

    , "feedback progress line": {
        "prefix": "nz-progress-line",
        "body": "<nz-progress [(ngModel)]=\"${1}\" [nzStatus]=\"'${2|success,exception,active|}'\" [nzType]=\"'line'\"\n     [nzStrokeWidth]=\"${3:10}\"></nz-progress>\n$0",
        "description": "进度条（线形）",
        "scope": ""
    }

    , "feedback spin": {
        "prefix": "nz-spin",
        "body": "<nz-spin${1: [nzTip]=\"'${2:加载中...}'\"}${3: [nzSize]=\"'${4|small,large,default|}'\"}${5: [nzSpinning]=\"false\"}></nz-spin>\n$0",
        "description": "加载中（用于页面和区块的加载中状态）",
        "scope": ""
    }

    , "attributes attr responsive": {
        "prefix": "nz-attr-responsive",
        "body": "[nzXs]=\"${1:1-24}\" [nzSm]=\"${2:1-24}\" [nzMd]=\"${3:1-24}\" [nzLg]=\"${4:1-24}\" [nzXl]=\"${5:1-24}\"",
        "description": "响应式属性",
        "scope": ""
    }

    , "attributes attr size": {
        "prefix": "nz-attr-size",
        "body": "[nzSize]=\"'${1|small,large,default|}'\"",
        "description": "大小属性",
        "scope": ""
    }

    , "attributes attr type": {
        "prefix": "nz-attr-type",
        "body": "[nzType]=\"'${1|primary,dashed,danger,default|}'\"",
        "description": "按钮类型属性",
        "scope": ""
    }  
}